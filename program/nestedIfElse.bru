
// nested if else example

method main(void){

 a = 6
 i = 2
 c = 3
 print("Value of c is ")
 print(c)
 print("Value of a is ")
 print(a)
 print("Value of i is ")
 print(i)
 if(c > i){
    if(c < a){
        print("The value of c is in between i and a")
    }else{
    print("Value of c is greater than both i and a")
    }

 }else{
 print("Value of c is less than both")
 }


}